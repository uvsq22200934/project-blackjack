paquet = ['2p', '3p', '4p', '5p', '6p', '7p', '8p', '9p','10p', 'Vp', 'Dp', 'Rp', 'Ap', \
    '2c', '3c', '4c', '5c' ,'6c' ,'7c' ,'8c' ,'9c' ,'10c' ,'Vc', 'Dc', 'Rc', 'Ac', \
