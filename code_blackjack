import random
import tkinter
from PIL import Image, ImageTk


"""listes et variables"""


"""p = pique, co = coeur, t = trèfle, ca = carreau"""
valeurs = {'2p': 2, '3p': 3, '4p': 4, '5p': 5, '6p': 6, '7p': 7, '8p': 8,
           '9p': 9, '10p': 10, 'Vp': 10, 'Dp': 10, 'Rp': 10, 'Ap': 11,
           '2co': 2, '3co': 3, '4co': 4, '5co': 5, '6co': 6, '7co': 7,
           '8co': 8, '9co': 9, '10co': 10, 'Vco': 10, 'Dco': 10, 'Rco': 10,
           'Aco': 11,
           '2t': 2, '3t': 3, '4t': 4, '5t': 5, '6t': 6, '7t': 7, '8t': 8,
           '9t': 9, '10t': 10, 'Vt': 10, 'Dt': 10, 'Rt': 10, 'At': 11,
           '2ca': 2, '3ca': 3, '4ca': 4, '5ca': 5, '6ca': 6, ' 7ca': 7,
           '8ca': 8, '9ca': 9, '10ca': 10, 'Vca': 10, 'Dca': 10, 'Rca': 10,
           'Aca': 11}

paquet = list(valeurs.keys())
defausse = []
main_joueur = []
main_croupier = []

bj21 = 0
money = 0


"""Fonctions"""


def debut(event):
    global name
    global money
    if event.widget == play:
        name = my_entry.get()
        money = my_entry2.get()
        if name == '':
            name = 'Joueur'
        if money == '':
            money = 1000
        accueil.destroy()
    if event.widget == partie_en_cours:
        money = my_entry2.get()
        name = my_entry.get()
        if name == '':
            name = 'Joueur'
        if money == '':
            money = 1000
        accueil.destroy()


def fenêtre():
    F1 = tkinter.Toplevel(accueil)


def donne_initiale():
    # fonction qui distribue les cartes en début de partie
    global paquet
    global main_joueur
    global main_croupier
    global carte_j1
    global carte_j2
    global carte_c1
    global bj21
    nb_carte = 0
    while nb_carte < 2:
        main_joueur.append(paquet[0])
        del paquet[0]
        main_croupier.append(paquet[0])
        del paquet[0]
        nb_carte += 1
    cj1.itemconfigure(carte_j1, text=main_joueur[0])
    cj2.itemconfigure(carte_j2, text=main_joueur[1])
    cc1.itemconfigure(carte_c1, text=main_croupier[0])
    disparition_mise()
    if score(main_joueur) == 21:
        resultat(action='blackjack')
        bj21 = 1
    print('main joueur', main_joueur, '\n main croupier', main_croupier)
    return main_joueur, main_croupier


def hit(main, pointeur):
    # fonction qui permet à un joueur de piocher un certain nombre de carte
    global main_joueur
    global main_croupier
    global paquet
    if pointeur == 'main_joueur':
        bouton_double_down.grid_forget()
        main.append(paquet[0])
        del paquet[0]
        if len(main) == 3:
            cj3.itemconfigure(carte_j3, text=main_joueur[2])
            cj3.grid(column=5, row=7)
        if len(main) == 4:
            cj4.itemconfigure(carte_j4, text=main_joueur[3])
            cj4.grid(column=6, row=7)
        if len(main) == 5:
            cj5.itemconfigure(carte_j5, text=main_joueur[4])
            cj5.grid(column=7, row=7)
        if len(main) == 6:
            cj6.itemconfigure(carte_j6, text=main_joueur[5])
            cj6.grid(column=8, row=7)
        if len(main) == 7:
            cj7.itemconfigure(carte_j7, text=main_joueur[6])
            cj7.grid(column=9, row=7)
        if len(main) == 8:
            cj8.itemconfigure(carte_j8, text=main_joueur[7])
            cj8.grid(column=10, row=7)
        if len(main) == 9:
            cj9.itemconfigure(carte_j9, text=main_joueur[8])
            cj9.grid(column=11, row=7)
        if len(main) == 10:
            cj10.itemconfigure(carte_j10, text=main_joueur[9])
            cj10.grid(column=12, row=7)
        if len(main) == 11:
            cj11.itemconfigure(carte_j11, text=main_joueur[10])
            cj11.grid(column=13, row=7)
        test_score(main)
    if pointeur == 'main_croupier':
        print(len(main))
        main.append(paquet[0])
        del paquet[0]
        if len(main_croupier) == 3:
            cc3.itemconfigure(carte_c3, text=main_croupier[2])
            cc3.grid(column=4, row=1)
        if len(main_croupier) == 4:
            cc4.itemconfigure(carte_c4, text=main_croupier[3])
            cc4.grid(column=5, row=1)
        if len(main_croupier) == 5:
            cc5.itemconfigure(carte_c5, text=main_croupier[4])
            cc5.grid(column=6, row=1)
        if len(main_croupier) == 6:
            cc6.itemconfigure(carte_c6, text=main_croupier[5])
            cc6.grid(column=7, row=1)
        if len(main_croupier) == 7:
            cc7.itemconfigure(carte_c7, text=main_croupier[6])
            cc7.grid(column=8, row=1)
        if len(main_croupier) == 8:
            cc8.itemconfigure(carte_c8, text=main_croupier[7])
            cc8.grid(column=9, row=1)
        if len(main_croupier) == 9:
            cc9.itemconfigure(carte_c9, text=main_croupier[8])
            cc9.grid(column=10, row=1)
        if len(main_croupier) == 10:
            cc10.itemconfigure(carte_c10, text=main_croupier[9])
            cc10.grid(column=11, row=1)
        if len(main_croupier) == 11:
            cc11.itemconfigure(carte_c11, text=main_croupier[10])
            cc11.grid(column=12, row=1)
    return main


def test_score(main):
    if score(main) >= 21:
        croupier()


def disparition_bouton_joueur():
    global bouton_hit
    print("bouton test", bouton_hit.grid_info())
    bouton_hit.grid_forget()
    stand_bouton.grid_forget()
    bouton_surrender.grid_forget()
    bouton_double_down.grid_forget()


def fin_manche():
    # fonction qui enlève les cartes des mains des joueurs et les place dans
    # la défausse
    global bj21
    global main_croupier
    global main_joueur
    global defausse
    disparition_bouton_joueur()
    while len(main_joueur) != 0:
        paquet.append(main_joueur[0])
        del main_joueur[0]
    while len(main_croupier) != 0:
        paquet.append(main_croupier[0])
        del main_croupier[0]
    print(len(paquet), paquet)
    print('mains croupier ', main_croupier, 'main joueur', main_joueur)
    bouton_rejouer.grid(column=2, row=3)
    random.shuffle(paquet)
    bj21 = 0
    return main_croupier, main_joueur


def score(main):
    # fonction qui permet de calculer la valeur d'une main
    points = 0
    Aca = 0
    Ap = 0
    Aco = 0
    At = 0
    for element in main:
        points += valeurs.get(element)
        print(points, valeurs.get(element), element)
    for element in main:
        while points > 21:
            if 'Aca' in element and Aca == 0:
                Aca = 1
                points -= 10
            if 'Ap' in element and Ap == 0:
                Ap = 1
                points -= 10
            if 'At' in element and At == 0:
                At = 1
                points -= 10
            if 'Aco' in element and Aco == 0:
                Aco = 1
                points -= 10
            else:
                break
    print('point final', points)
    return points


def croupier():
    # fonction qui définit les action du croupier en fonction de sa main
    global main_croupier
    cc2['bg'] = 'white'
    cc2.itemconfigure(carte_c2, text=main_croupier[1])
    cc2.grid(column=3, row=1)
    points_croupier = score(main_croupier)
    while score(main_croupier) <= 16:
        print(points_croupier, main_croupier)
        hit(main_croupier, 'main_croupier')
    resultat()
    return main_croupier, points_croupier


def stand(main, bouton=0):
    # fonction qui permet au joueur de "stand"
    points = score(main)
    if bouton == 'stand_bouton':
        stand_label.grid(column=0, row=5)
        print("je m'arrête")
        cc2['bg'] = 'white'
        cc2.itemconfig(carte_c2, text=main_croupier[1])
        cc2.grid(column=3, row=1)
        croupier()
    return points


def doubble_down(main, mise=0):
    # fonction qui permet au joueur de "double down"
    mise *= 2
    hit(main, 'main_joueur')
    print(main)
    points_joueur = score(main)
    croupier()
    return points_joueur


def surrender(mise=0):
    # fonction qui permet au joueur de "surrender"
    mise /= 2
    resultat(action='surrender')
    return mise


def resultat(action=0):
    # fonction qui donne le résulat du jeu
    global main_joueur
    global main_croupier
    if action == 'blackjack':
        score__joueur['text'] = score(main_joueur)
        score__joueur.grid(column=9, row=1)
        resultat_label['text'] = 'BLACKJACK: vous êtes le vainqueur'
        resultat_label.grid(column=10, row=2)
    elif action == 'surrender':
        score__joueur['text'] = score(main_joueur)
        score__joueur.grid(column=9, row=1)
        resultat_label['text'] = 'Le CROUPIER est le vainqueur'
    elif score(main_joueur) > 21:
        annonce = 'Votre score: ' + str(score(main_joueur))
        resultat_final['text'] = annonce
        resultat_final.grid(column=9, row=1)
        resultat_label['text'] = 'Votre main vaut plus que 21, vous avez\
            perdu.'
        resultat_label.grid(column=10, row=2)
    elif score(main_joueur) <= 21:
        if score(main_croupier) > 21:
            annonce = 'Votre score: ' + \
                str(score(main_joueur)) + '\n Score du croupier: ' + \
                str(score(main_croupier))
            resultat_final['text'] = annonce
            resultat_final.grid(column=9, row=1)
            resultat_label['text'] = 'Le croupier a dépassé 21: vous avez\
                gagné !'
            resultat_label.grid(column=10, row=2)
        elif score(main_croupier) <= 21:
            if score(main_joueur) < score(main_croupier):
                annonce = 'Votre score: ' + \
                    str(score(main_joueur)) + '\n Score du croupier: ' + \
                    str(score(main_croupier))
                resultat_final['text'] = annonce
                resultat_final.grid(column=9, row=1)
                resultat_label['text'] = 'Le croupier a un meilleur score,\
                    vous avez perdu.'
                resultat_label.grid(column=10, row=2)
            elif (score(main_joueur)) == score(main_croupier):
                annonce = 'Votre score: ' + \
                    str(score(main_joueur)) + '\n Score du croupier: ' + \
                    str(score(main_croupier))
                resultat_final['text'] = annonce
                resultat_final.grid(column=9, row=1)
                resultat_label['text'] = 'Vous avez le même score que le\
                    croupier: ex aequo'
                resultat_label.grid(column=10, row=2)
            elif score(main_joueur) > score(main_croupier):
                annonce = 'Votre score: ' + \
                    str(score(main_joueur)) + '\n Score du croupier: ' + \
                    str(score(main_croupier))
                resultat_final['text'] = annonce
                resultat_final.grid(column=9, row=1)
                resultat_label['text'] = 'Vous avez un meilleur score que le\
                    croupier: vous avez gagné !'
                resultat_label.grid(column=10, row=2)
    print(score(main_joueur), score(main_croupier))
    fin_manche()


