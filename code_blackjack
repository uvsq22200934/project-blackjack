def quitter(event):
    if event.widget == bouton_quitter:
        root.destroy()


def ordre_boutons2(event):
    if event.widget == bouton_melanger:
        bouton_melanger.grid_forget()
        bouton_donne_initiale.grid(column=2, row=3)


def ordre_boutons3(event):
    global bj21
    if event.widget == bouton_donne_initiale:
        print('ok')
        cc1['bg'] = 'white'
        cc2['bg'] = 'red4'
        cj1['bg'] = 'white'
        cj2['bg'] = 'white'
        donne_initiale()
        bouton_donne_initiale.grid_forget()
        if bj21 == 0:
            apparition_bouton_joueur()


def apparition_bouton_joueur():
    bouton_hit.grid(column=2, row=3)
    bouton_double_down.grid(column=2, row=4)
    stand_bouton.grid(column=2, row=5)
    bouton_surrender.grid(column=2, row=6)


def apparition_mise():
    jeton_vert.grid(column=3, row=9)
    valeur_vert.grid(column=3, row=9)
    jeton_bleu.grid(column=2, row=9)
    jeton_rouge.grid(column=1, row=9)
    jeton_noir.grid(column=5, row=9)
    miser.grid(column=0, row=9)


def disparition_mise():
    jeton_vert.grid_forget()
    valeur_vert.grid_forget()
    jeton_bleu.grid_forget()
    jeton_rouge.grid_forget()
    jeton_noir.grid_forget()
    miser.grid_forget()


def mise_rouge(event):
    global mise_totale
    if event.widget == jeton_rouge:
        mise_totale += 10
        valeur_mise['text'] = mise_totale
    return mise_totale


def mise_bleu(event):
    global mise_totale
    if event.widget == jeton_bleu:
        mise_totale += 25
        valeur_mise['text'] = mise_totale
    return mise_totale


def mise_vert(event):
    global mise_totale
    if event.widget == jeton_vert:
        mise_totale += 50
        valeur_mise['text'] = mise_totale
    return mise_totale


def mise_noir(event):
    global mise_totale
    if event.widget == jeton_noir:
        mise_totale += 100
        valeur_mise['text'] = mise_totale
    return mise_totale


